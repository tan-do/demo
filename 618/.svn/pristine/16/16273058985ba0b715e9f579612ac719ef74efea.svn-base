<script>
    var app = new Vue({
        el: '#app',
        data: {
            //主会场列表
            huicangList: [
                {'img':'./img/index/hc1.png','url':'https://s.click.taobao.com/8eD8VQw'},
                {'img':'./img/index/hc2.png','url':'https://s.click.taobao.com/0x9jSQw'}
            ],
            //分类区块列表
            fenleiList: [
                {'img':'./img/index/qk1.png','url':'https://s.click.taobao.com/3MtKRQw'},
                {'img':'./img/index/qk2.png','url':'https://s.click.taobao.com/RscKRQw'},
                {'img':'./img/index/qk3.png','url':'https://s.click.taobao.com/dQRORQw'},
                {'img':'./img/index/qk4.png','url':'https://s.click.taobao.com/7LqNRQw'},
                {'img':'./img/index/qk5.png','url':'https://s.click.taobao.com/9RjCRQw'},
                {'img':'./img/index/qk6.png','url':'https://s.click.taobao.com/8F0ORQw'},
                {'img':'./img/index/qk7.png','url':'https://s.click.taobao.com/UocCRQw'},
                {'img':'./img/index/qk8.png','url':'https://s.click.taobao.com/WpWJRQw'},
                {'img':'./img/index/qk9.png','url':'https://s.click.taobao.com/U51CRQw'},
                {'img':'./img/index/qk10.png','url':'https://s.click.taobao.com/lbpBRQw'}
            ],
            //百强活动列表
            huodongList: [
                {'img':'./img/index/hd1.png','url':'/huodong/618/2018/mobile/miaosha.html'},
                {'img':'./img/index/hd2.png','url':'/huodong/618/2018/mobile/chaopianyi.html'},
                {'img':'./img/index/hd3.png','url':'/huodong/618/2018/mobile/baokuan.html'},
                {'img':'./img/index/hd4.png','url':'/huodong/618/2018/mobile/yushou.html'},
            ],
            //百强活动类别列表
            hdlbList: [
                {'img':'./img/index/tp1.png','url':'https://m.qiang100.com/zhi/youhuiquan/'},
                {'img':'./img/index/tp2.png','url':'https://m.qiang100.com/pinpai/'},
                {'img':'./img/index/tp3.png','url':'https://m.qiang100.com/zhi/'},
                {'img':'./img/index/tp4.png','url':'https://m.qiang100.com/index.php?g=&m=Zhi&a=history'},
            ],
            mshzList:[],    //秒杀汇总
            mshzLoad:false,
            cjpyList:[],    //超级便宜
            cjpyLoad:false,
            bkqdList:[],    //爆款清单
            bkqdLoad:false,
            yshjList:[],     //预售好价
            yshjLoad:false,
        },
        created: function () {
            var vm = this;
            window.onscroll = function () {
                vm.scroll();
            }
        },
        methods: {
            //滚动加载
            scroll() {
                var vm = this;
                var _nowPos = $(window).scrollTop();
                if(_nowPos > 400){
                    vm.loadMiaosha();
                }
                if(_nowPos > 800){
                    vm.loadPianyi();
                }
                if(_nowPos > 1200){
                    vm.loadBaokuan();
                }
                if(_nowPos > 1600){
                    vm.loadYushou();
                }
            },
            //秒杀汇总
            loadMiaosha:function () {
                var vm = this;
                var _url = '/Plugapi/popupPage/getListOfChina';
                if(vm.mshzLoad == false){
                    vm.mshzLoad = true;
                    $.get(_url ,function (data) {
                        if(data.code == 100){
                            vm.mshzList = data.data.productList.slice(0,4);
                            console.log();
                        }else{
                            console.log('err1');

                        }
                    })
                }
            },
            //超级便宜
            loadPianyi:function () {
                var vm = this;
                var _url = '/Plugapi/popupPage/getListOfChina';
                if(vm.cjpyLoad == false){
                    vm.cjpyLoad = true;
                    $.get(_url ,function (data) {
                        if(data.code == 100){
                            vm.mshzList = data.data.productList.slice(0,4);
                            console.log();
                        }else{
                            console.log('err1');

                        }
                    })
                }
            },
            //爆款清单
            loadBaokuan:function () {
                var vm = this;
                var _url = '/Plugapi/popupPage/getListOfChina';
                if(vm.bkqdLoad == false){
                    vm.bkqdLoad = true;
                    $.get(_url ,function (data) {
                        if(data.code == 100){
                            vm.mshzList = data.data.productList.slice(0,4);
                            console.log();
                        }else{
                            console.log('err1');

                        }
                    })
                }
            },
            //预售好价
            loadYushou:function () {
                var vm = this;
                var _url = '/HuodongApi/hd618/hd2018_yshj';
                if(vm.yshjLoad == false){
                    vm.yshjLoad = true;
                    $.get(_url ,function (data) {
                        if(data.code == 100){
                            vm.yshjList = data.data.productList.slice(0,8);
                            console.log();
                        }else{
                            console.log('err1');

                        }
                    })
                }
            },
        }
    });
</script>
