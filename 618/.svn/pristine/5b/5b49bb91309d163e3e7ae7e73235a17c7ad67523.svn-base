<script>

    /*顶部收藏及关闭*/
    $(function(){

        var topTipCookieKey = 'rSeDyn_' + $.cookie('rSeDyn_CXID') + '_topTipForBookmarks';
        if(! $.cookie(topTipCookieKey) ){
            $('#tt_ab').show();
        }else{
            $('#tt_ab').hide();
        }

        $("#tt_ab .top-tip-close").click(function(){
            $('#tt_ab').animate({
                height:'0px',opacity:'0.0'
            },'slow', null, function () {
                $('#tt_ab').addClass('hidden');
            });
            $.cookie( topTipCookieKey, 1, {expires:1, path:'/'} );
        });

        //监听ctrl+d按键
        var lastCtrlDKey = 0;
        $(document).bind('keydown', function(event) {
            var key = event.keyCode;
            if (key == 68 && (lastCtrlDKey==91||lastCtrlDKey==17)) {
                addBaiduEvent(['top-add-bookmarks', 'top-add-bookmarks', document.title]);
                console.log('press ctrl+d');
            }
            lastCtrlDKey = key;
            return true;
        });
    });


    //自定义统计
    function sts(type, key, id) {
        $.get("/index.php?g=api&m=statistics&a=touch", {type: type, key: key, id: id}, function(res){
            console.log(res);
        },"json");
    }

    //页面访问统计
    $(function(){
        sts('diy_2017_1111_visit', 'visit_count', window.location.host);
        if (isMobile()) {
            sts('diy_2017_1111_visit_mobile', 'visit_count', window.location.host);
        }
    });
    var domain = window.location.host.split(':')[0];
    if (/^127\.|^192\.|^10\./.test(domain) === false) {
        domain = domain.substr(domain.indexOf('.'))
    }
</script>



