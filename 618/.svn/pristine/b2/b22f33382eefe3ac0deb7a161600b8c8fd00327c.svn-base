}
.refresh {
    font-size: 12px;
    color: #999
}
.twoWeeks .ckeckBox {
    vertical-align: middle;
    margin-right: 9px;
    color: #999;
    margin: 0 5px 0 0
}
.twoWeeks label {
    padding-right: 26px;
    cursor: pointer;
    color: #666
}
.buy a, .btn_login, .btn_reg, a.a_redBlock, a.a_greyBlock, .btn_grey {
    display: block;
    width: 110px;
    height: 40px;
    line-height: 40px;
