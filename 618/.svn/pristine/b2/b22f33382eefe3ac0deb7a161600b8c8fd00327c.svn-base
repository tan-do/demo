    color: #fff;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    -ms-border-radius: 2px;
    -o-border-radius: 2px;
    border-radius: 2px;
    font-size: 14px
}
.btn_login, .btn_reg, .btn_grey {
    border: 0;
    width: 150px;
    padding-left: 0;
    margin: 0 auto 20px;
    cursor: pointer
}
input[type="submit"], input[type="reset"], input[type="button"], button {
    -webkit-appearance: none
}
.co_login {
    text-align: center;
    color: #999
}
.co_login a {
    padding-left: 12px
}
.captcha_imgBox {
    width: 50%;
    border: 0;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -ms-border-radius: 5px;
    -o-border-radius: 5px;
    border-radius: 5px;
