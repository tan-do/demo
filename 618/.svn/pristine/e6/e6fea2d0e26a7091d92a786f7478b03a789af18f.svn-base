//秒杀页面
.miaosha-banner {
    width: 100%;
    height: 480px;
    min-width: 1200px;
    background: #6900FE url('../img/miaosha-banner.png') no-repeat center;
    float: left;
    cursor: default;
    .miaosha-time{
        width: 465px;
        height: 168px;
        background: #FF1800;
        border-radius: 5px;
        margin: 0 auto;
        margin-top:280px;
        border:2px solid #fff;
        p{position: relative;top:10px;}
        p:first-child{width: 100%;height: 50px;background: transparent;font-size: 29px;color: #fff;text-align: center;line-height: 50px;}
        p:last-child{width: 100%;height: 100px;box-sizing: border-box;padding: 0 48px;
            span {
                width: 68px;
                height: 68px;
                display: block;
                background: #FFF9DF;
                float: left;
                border-radius: 5px;
                text-align: center;
                line-height: 68px;
                font-size: 45px;
                color: #333;
            }
            em{display: block;float: left;width: 22px;height: 68px;background: red;line-height: 68px;text-align: center;
                font-size: 29px;color: #fff;font-style: normal;
            }
            .em-1{width: 48px;height: 68px;}
        }
    }

}
.miaosha-img {
    width: 100%;
    height: 106px;
    float: left;
    background: #6900FE;
}
.miaosha-content {
    width: 100%;
    height: auto;
    min-width: 1200px;
    padding-top: 20px;
    padding-bottom: 50px;
    background: #6900FE;
    float: left;
}


.tab-link {
    width: 100%;
    height: 46px;
    background: #9C3FE3;
    float: left;
    ul {
        width: 100%;
        height: 46px;
        background: #D093FF;
        display: flex;
        li{flex:1;text-align: center;height: 46px;line-height: 46px;font-size:18px;}
        li.active{
            background: #fff;
        }
        li:hover{background: #fff;cursor: pointer;}

    
    }

}

.tab-yushou{
    background: #F70200;
    ul {
        background: #FF5E5C;
        li{color:#9D0100 };
        li:hover{color: #FC0300}
    }
}


.side-bar {
    width: 240px;
    height: 100%;
    background: rgba(24, 24, 24, .9);
    position: fixed;
    left: 0;
    top:0;
    opacity: .9;
    ul{width: 100%;height: auto;
        li:first-child{width: 100%;height: 180px;/* background:  url('../img/sidebar-1.png'); */
            text-align: center;margin-top:0;
            a{position: absolute;left: 50%;top:120px;margin-left: -75px;
                img:hover{opacity: .8;}
            }
            img:hover{opacity: 1;}
        }
        li{width: 100%;height: auto;margin:10px auto;/* background: pink; */display: block;text-align: center;position: relative;
            
            a{
                img:hover{opacity: .8;}
            }
        }
    }

    i.shouqi{position: absolute;right: -30px;top:0;}
}


.side-bar-show{width: 158px;height: 115px;background: #333;position: fixed;left: 0;top:50%;margin-top:-79px;
    display: none;
    i.zhankai {
        position: absolute;
        right: -30px;
        top: 0;
    }

}
/* 公用底部 */


#bigAutocompleteContent {
    overflow: visible;
}

#bigAutocompleteContent>.searchTipLine {
    height: 40px;
    margin: 0;
    padding: 6px 0 6px 0;
}

#bigAutocompleteContent>.searchTipLine>dd {
    color: #888888;
}

#tt_ab {
    height: 42px;
    background: #f6f6f6;
    color: #008aca;
    overflow: hidden;
    margin: 0;
    line-height: 42px;
    display: none;
}

#tt_ab>.text-center {
    color: #333;
}

#tt_ab>.text-center>.key-btn {
    display: inline-block;
    padding: 3px 5px;
    background-color: #dd2727;
    color: #FFFFFF;
    line-height: 14px;
    font-size: 14px;
    border-radius: 3px;
}

#tt_ab>.text-center>.top-tip-close {
    color: #9D9D9D;
    cursor: pointer;
    line-height: 38px;
    font-size: 26px;
    margin-right: 20px;
}

#tt_ab>.text-center>.tel {
    float: right;
    margin-right: 40px;
    color: #999;
}

#tt_ab .red {
    color: #dd2727;
}
#foot {
    width: 100%;
    height: 120px;
    float: left;
}
#foot .container {
    font-size: 13px;
    line-height: 20px;
    color: #7a7a7a;
    min-height: 120px;
}

#foot .container a {
    color: #7a7a7a;
}

#foot .container .friend-link-line {
    padding-bottom: 10px;
    overflow: hidden;
    height: 22px;
}

#foot .container .friend-link-line a {
    margin: 0 5px 0 5px;
}

#foot .container .nav-link-line {
    padding-bottom: 15px;
}

#foot .container .nav-link-line .split-char {
    margin: 0 10px 0 10px;
    color: #e1e1e1;
}

#foot .container .copyright-line {
    padding-bottom: 20px;
    position: relative;
    top:15px;
}
