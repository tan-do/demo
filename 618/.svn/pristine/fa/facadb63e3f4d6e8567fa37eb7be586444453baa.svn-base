}

.top-nav-2 {
    width: 7.26rem;
    overflow: hidden;
    padding-left: 0.24rem;
    padding-top: 0.04rem;
}

.top-nav-2 img {
    width: 1.34rem;
    height: 1.74rem;
    float: left;
    margin-left: 0.06rem;
    margin-top: 0.12rem;
}

.main-box {
    width: 7.5rem;
}

.main-box .head1 {
    width: 7.5rem;
    text-align: center;
    height: 0.67rem;
    margin: 0.7rem auto 0.25rem;
}

.main-box .head1 img {
    height: 0.67rem;
}

.main-box .more {
    width: 7.5rem;
    text-align: center;
    height: 0.78rem;
    margin: 0.7rem auto 0.25rem;
}

.main-box .more img {
    width: 3.6rem;
    height: 0.78rem;
}

.main-box1 .ul1 {
    width: 7.3rem;
    overflow: hidden;
    padding-left: 0.2rem;
    padding-top: 0.04rem;
}

.main-box1 .ul1 img {
    width: 3.41rem;
    height: 2.06rem;
    float: left;
    margin-left: 0.1rem;
    margin-top: 0.12rem;
}

.main-box1 .ul2 {
    width: 7.27rem;
    overflow: hidden;
    padding-left: 0.23rem;
    padding-top: 0.04rem;
}

.main-box1 .ul2 img {
    width: 1.7rem;
    height: 2.26rem;
    float: left;
    margin-left: 0.06rem;
    margin-top: 0.1rem;
}

.main-box2 ul {
    width: 7.36rem;
    overflow: hidden;
    padding-left: 0.14rem;
}

.main-box2 ul li {
    width: 3.34rem;
    background-color: #fff;
    float: left;
    margin: 0.2rem 0 0 0.18rem;
    line-height: 0.38rem;
