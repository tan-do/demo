}

.main-box2 ul li a img {
    width: 3.34rem;
    height: 3.34rem;
}

.main-box2 ul li a .box-in {
    width: 2.94rem;
    padding: 0.2rem;
}

.main-box2 ul li a .title {
    font-size: 0.24rem;
    color: #333;
    max-width: 2.94rem;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-weight: bold;
}

.main-box2 ul li a .box-in1 .price {
    font-size: 0.24rem;
    color: #333;
    font-weight: bold;
}

.main-box2 ul li a .box-in1 .price span {
    font-weight: normal;
    color: #FF0100;
}

.main-box2 ul li a .box-in1 .time {
    font-size: 0.24rem;
    color: #666;
}

.main-box2 ul li a .box-in2 .price {
    font-size: 0.24rem;
    color: #FF0100;
    font-weight: bold;
    margin-top: 0.1rem;
}

.main-box2 ul li a .box-in2 .price span {
    font-weight: normal;
    font-size: 0.22rem;
    color: #22C051;
    float: right;
}

.main-box2 ul li a .box-in3 .price {
    font-size: 0.24rem;
    color: #FF0100;
    font-weight: bold;
    margin-top: 0.1rem;
}

.main-box2 ul li a .box-in3 .price span {
    font-weight: normal;
    font-size: 0.22rem;
    color: #999999;
}

.main-box2 ul li a .box-in3 .coupon {
    width: 100%;
    overflow: hidden;
    margin-top: 0.2rem;
}

.main-box2 ul li a .box-in3 .coupon-l {
    width: 1.3rem;
    height: 0.52rem;
    text-align: center;
    line-height: 0.52rem;
    font-size: 0.28rem;
    color: #fff;
    float: left;
    background: url(../img/coupon-bg.png) no-repeat;
    background-size: 100%;
}

.main-box2 ul li a .box-in3 .coupon-r {
    width: 1.45rem;
    height: 0.52rem;
    text-align: center;
    line-height: 0.52rem;
    font-size: 0.28rem;
    color: #fff;
    background-color: #FF0100;
    float: right;
}

.main-box2 ul li a .box-in4 .price {
    font-size: 0.24rem;
    color: #FF0100;
    font-weight: bold;
}

.main-box2 ul li a .box-in4 .jian {
    font-size: 0.24rem;
    color: #333333;
}

.main-box2 ul li a .button {
    font-size: 0.28rem;
    color: #fff;
    width: 2.94rem;
    height: 0.6rem;
    line-height: 0.65rem;
    text-align: center;
    background-color: #ff0100;
    margin-top: 0.18rem;
}

/* 首页弹窗 */

.layer {
    width: 7.5rem;
    height: calc(100% - 0.98rem);
    background: rgba(51, 51, 51, .5);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    left: 50%;
    margin-left: -3.75rem;
    bottom: 1rem;
}

.layer-content {
    width: 100%;
    height: 5rem;
    background: #fff;
    position: absolute;
    bottom: 0;
    border-bottom: 1px solid #ddd;
}

.layer-link {
    width: 7.1rem;
    height: 3.6rem;
    /*  background: red; */
    margin: .3rem auto;
}

.layer-link p {
    width: 100%;
    height: .3rem;
    /* background: #333; */
    margin-bottom: .2rem;
    display: flex;
}

.layer-link p:first-child {
    height: 1.6rem;
}

.layer-link p:nth-child(2) {
    height: 1.16rem;
}

.layer-link p:last-child {
    height: 1.16rem;
}

.layer-link p a {
    height: 100%;
    display: block;
    float: left;
    margin-right: .2rem;
}

.layer-link p a img:hover,
.layer-link p a img:active {
    opacity: .8;
}

.layer-link p a:last-child {
    margin-right: 0;
}

.layer-link p a img {
    height: 100%;
}

i.layer-icon {
    width: .6rem;
    height: .6rem;
    background: red;
    position: absolute;
    bottom: 5.3rem;
    left: 50%;
    margin-left: -.3rem;
    background: url('../img/index/tc_close.png') no-repeat center;
    background-size: 100%;
}

/*公共底部*/

.bottom-nav {
    position: fixed;
    bottom: 0;
    left: 50%;
    margin-left: -3.75rem;
    width: 7.5rem;
    height: 0.98rem;
    background-color: #fff;
    z-index: 9999;
}

.bottom-nav ul li {
    width: 2.5rem;
    height: 0.98rem;
    border-right: 1px #BDB4B7 solid;
    font-size: 0.32rem;
    color: #666;
}

.bottom-nav ul li i {
    margin-right: 0.1rem;
}

.bottom-nav ul li i.hc {
    display: inline-block;
    width: 0.3rem;
    height: 0.24rem;
    line-height: inherit;
    color: #fff;
    background: url(../img/hc-icon.png) no-repeat;
    background-size: 100%;
}

.bottom-nav ul li i.app {
    display: inline-block;
    width: 0.48rem;
    height: 0.48rem;
    line-height: inherit;
    color: #fff;
    background: url(../img/app-logo.png) no-repeat;
    background-size: 100%;
}

.bottom-nav ul li.morehc {
    position: relative;
}

.bottom-nav ul li .hc-more {
    width: 2rem;
    height: 2rem;
    line-height: 0.5rem;
    text-align: center;
    padding-top: 0.3rem;
    position: absolute;
    bottom: 0.98rem;
    left: 0.25rem;
    background-color: #fff;
    display: none;
    z-index: 1000;
}

.bottom-nav ul li:last-child {
    border: 0;
}

#share {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 1000;
    display: none;
    width: 7.5rem;
    height: 3.5rem;
    background-color: #fff;
}

ul.share {
    width: 6rem;
    padding: 0.2rem 0.75rem;
}

ul.share li {
    width: 2rem;
    float: left;
    text-align: center;
    font-size: 0.24rem;
    color: #999;
    margin-top: 0.2rem;
}

ul.share li img {
    width: 1rem;
    height: 1rem;
}

